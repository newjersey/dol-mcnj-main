{
  "name": "next-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:only": "npm --prefix backend run build && next build",
    "build": "npm --prefix backend run build && npm --prefix backend run start & wait-on tcp:8080 && next build",
    "build:prod": "npm --prefix backend run build && next build",
    "start:backend": "cd backend && node ./dist/server.js",
    "start:frontend": "next start",
    "check": "prettier --check .",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "concurrently \"next dev --turbopack\" \"cd backend/ && npm run start:dev\"",
    "format": "prettier --write .",
    "install-all": "concurrently \"npm install\" \"cd backend/ && npm install\"",
    "lint": "next lint",
    "start": "next start",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "jest"
  },
  "dependencies": {
