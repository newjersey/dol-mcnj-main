{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node build.js",
    "lint": "eslint --max-warnings 0 '*/**/*.{js,ts}'",
    "lint:fix": "eslint --max-warnings 0 '*/**/*.{js,ts}' --fix",
    "start": "npm run db-migrate up -- -e $DB_ENV && node ./dist/server.js --env=production",
    "start:dev": "nodemon",
    "test": "jest",
    "db-migrate": "db-migrate",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "fences": "gf",
    "start:wiremock": "wiremock --root-dir ./wiremock --port 8090"
  },
  "prettier": {
    "printWidth": 100
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "exec": "ts-node ./src/server.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "knex": "^0.21.2",
    "nodemon": "^2.0.4",
    "papaparse": "^5.2.0",
    "pg": "^8.2.1",
    "tslib": "^2.0.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.6",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.0.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "fs-extra": "^10.0.1",
    "good-fences": "^0.9.1",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "supertest": "^6.2.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "wiremock": "^2.27.1"
  },
  "engines": {
    "node": "^16.0.0"
  }
